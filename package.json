{
  "name": "More-Recipes",
  "version": "1.0.0",
  "description": "More-Recipes is a Recipe Web Application, it could be thought of as an online meet-up for everyone that enjoys cooking and sharing recipes, or for those who are probably just interested in finding out how amazing cuisines are put together. Users can either Sign Up to enjoy some awesome features or view recipes as a visitor.\r # Authors\r Rachel Abaniwo\r # Contributors\r # Ackwoledgements",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon server/app.js --exec babel-node",
    "babel-node": "babel-node --",
    "start": "cross-env node dist/app.js",
    "build": "./node_modules/.bin/babel server -d dist --copy-files",
    "cleardb": "NODE_ENV=test sequelize db:migrate:undo:all",
    "pretest": "npm run cleardb && NODE_ENV=test sequelize db:migrate && NODE_ENV=test sequelize db:seed:all",
    "test": "cross-env NODE_ENV=test mocha server/tests/**/**.spec.js --compilers js:babel-core/register --exit",
    "test-watch": "nodemon --exec \"npm test\"",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls",
    "heroku-postbuild": "npm run build:client && npm run build",
    "start:client": "./node_modules/.bin/webpack-dev-server",
    "test:client": "./node_modules/.bin/jest --watch --coverage",
    "build:client": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RachelAbaniwo/More-Recipes.git"
